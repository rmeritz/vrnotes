<h1>Annotations</h1>

<table>
  <tr>
    <th>Coordinate</th>
    <th>Annotation</th>
    <th></th>
  </tr>
  <% @annotations.each do |annotation| %>
    <tr>
      <td><%= annotation.coordinate %></td>
      <td><%= annotation.Annotation %></td>
      <td><%= link_to "Edit", edit_annotation_path(annotation) %></td>
    </tr>
  <% end %>
</table>
